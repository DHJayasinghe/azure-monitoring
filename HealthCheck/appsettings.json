{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecks-UI": {
    "HealthChecks": [
      {
        "Name": "Service1",
        "Uri": "https://Service1.azurewebsites.net/hc"
      },
      {
        "Name": "Service2",
        "Uri": "https://Service2.azurewebsites.net/hc"
      }
    ],
    "EvaluationTimeInSeconds": 15,
    "MinimumSecondsBetweenFailureNotifications": 30,
    "MaximumHistoryEntriesPerEndpoint": 10
  }
}
